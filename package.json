{
  "name": "@elizaos-plugins/plugin-vercel-ai-gateway",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "description": "Universal AI Gateway integration plugin for elizaOS - Access 100+ AI models through Vercel AI Gateway and other unified gateways",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elizaos-plugins/plugin-vercel-ai-gateway.git"
  },
  "homepage": "https://github.com/elizaos-plugins/plugin-vercel-ai-gateway#readme",
  "bugs": {
    "url": "https://github.com/elizaos-plugins/plugin-vercel-ai-gateway/issues"
  },
  "keywords": [
    "elizaos",
    "plugin",
    "ai",
    "gateway",
    "vercel",
    "openrouter",
    "multi-provider"
  ],
  "author": "elizaOS Community",
  "license": "MIT",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "plugin.json"
  ],
  "dependencies": {
    "@ai-sdk/openai": "^1.3.20",
    "@elizaos/core": "^1.0.0",
    "@elizaos/plugin-anthropic": "^1.0.0-beta.54",
    "@elizaos/plugin-bootstrap": "^1.2.11-beta.9",
    "@elizaos/plugin-knowledge": "^1.0.0-beta.78",
    "@elizaos/plugin-openai": "^1.0.0-beta.74",
    "@elizaos/plugin-telegram": "^1.0.0-beta.55",
    "@elizaos/plugin-twitter": "^1.0.0-beta.57",
    "ai": "^4.3.16",
    "lru-cache": "^10.0.0",
    "p-retry": "^6.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "prettier": "3.5.3",
    "tsup": "8.5.0",
    "typescript": "^5.8.2",
    "vitest": "^1.0.0"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --tsconfig ./tsconfig.json",
    "dev": "tsup src/index.ts --format esm --dts --watch --tsconfig ./tsconfig.json",
    "test": "npx elizaos test",
    "lint": "prettier --write ./src",
    "clean": "rm -rf dist .turbo node_modules",
    "format": "prettier --write ./src",
    "prepare-submission": "node scripts/prepare-submission.js",
    "check": "npm run format && npm run build && npm test"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {
    "@elizaos/core": "^1.0.0"
  },
  "agentConfig": {
    "pluginType": "elizaos:plugin:1.0.0",
    "pluginParameters": {
      "AIGATEWAY_API_KEY": {
        "type": "string",
        "description": "API key for AI Gateway",
        "required": false,
        "sensitive": true
      },
      "AIGATEWAY_BASE_URL": {
        "type": "string",
        "description": "Base URL for AI Gateway (Vercel, OpenRouter, etc)",
        "required": false,
        "default": "https://ai-gateway.vercel.sh/v1",
        "sensitive": false
      },
      "AIGATEWAY_DEFAULT_MODEL": {
        "type": "string",
        "description": "Default model to use",
        "required": false,
        "default": "openai:gpt-4o-mini",
        "sensitive": false
      },
      "AIGATEWAY_LARGE_MODEL": {
        "type": "string",
        "description": "Large model to use",
        "required": false,
        "default": "openai:gpt-4o",
        "sensitive": false
      },
      "AIGATEWAY_EMBEDDING_MODEL": {
        "type": "string",
        "description": "Embedding model to use",
        "required": false,
        "default": "openai:text-embedding-3-small",
        "sensitive": false
      },
      "AIGATEWAY_CACHE_TTL": {
        "type": "number",
        "description": "Cache TTL in seconds",
        "required": false,
        "default": 300,
        "sensitive": false
      },
      "AIGATEWAY_MAX_RETRIES": {
        "type": "number",
        "description": "Maximum retry attempts",
        "required": false,
        "default": 3,
        "sensitive": false
      }
    }
  }
}
